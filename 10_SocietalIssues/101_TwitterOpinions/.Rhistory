g2 <- ggplot(aes(x=Twitter2, y=result), data=df) + geom_point() + geom_text(label=df$Party, hjust = "inward", nudge_x = c(0.3,rep(0.5,5),-0.5)) + theme_bw() + xlab("Twitter prediction 2 (%)") + ylab("Election result")+ xlim(c(2,35)) + geom_smooth(method = "lm", se = F, col=rgb(0,0,0,0.25))
g1 + g2
